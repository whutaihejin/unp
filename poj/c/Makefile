.PHONY : ALL
ALL :  1000 1002 1004 1005 1006

1000:
	gcc -g -Wall -std=c99 -o 1000 1000.c -lm
1002:
	gcc -g -Wall -std=c99 -o 1002 1002.c -lm
1004:
	gcc -g -Wall -std=c99 -o 1004 1004.c -lm
1005:
	gcc -g -Wall -std=c99 -o 1005 1005.c -lm
1006:
	gcc -g -Wall -std=c99 -o 1006 1006.c -lm

.PHONY : clean
clean : 
	 rm -rf  1000 1002 1004 1005 1006
